# Nutrition App Environment Variables

# App Configuration
VITE_APP_NAME=Nutrition & Meal Planning App
VITE_APP_ENV=development

# Database Configuration (IndexedDB - local only)
VITE_DB_NAME=NutritionDB
VITE_DB_VERSION=1

# Optional: External APIs (for future integrations)
# VITE_BARCODE_API_KEY=your_barcode_api_key_here
# VITE_NUTRITION_API_KEY=your_nutrition_api_key_here

# Feature Flags
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_PERSISTENT_STORAGE=true
